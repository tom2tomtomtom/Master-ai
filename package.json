{
  "name": "master-ai-saas",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "prebuild": "prisma generate",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "security-audit": "npm audit --audit-level moderate",
    "quality-check": "npm run type-check && npm run lint && npm run security-audit",
    "import-content": "tsx scripts/import-content.ts",
    "import-content:clear": "tsx scripts/import-content.ts --clear",
    "validate-content": "tsx scripts/validate-content.ts",
    "setup": "tsx scripts/setup.ts",
    "seed-achievements": "tsx scripts/seed-achievements.ts",
    "setup-stripe-products": "tsx scripts/setup-stripe-products.ts",
    "production-setup": "tsx scripts/production-setup.ts",
    "production-setup:dry-run": "tsx scripts/production-setup.ts --dry-run",
    "validate-production": "tsx scripts/validate-production.ts",
    "deploy-staging": "tsx scripts/deploy-staging.ts",
    "validate-staging": "tsx scripts/validate-staging.ts",
    "db:migrate:prod": "prisma migrate deploy",
    "db:reset:prod": "prisma migrate reset --force",
    "postbuild": "prisma generate",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:full": "tsx src/__tests__/test-runner.ts",
    "test:critical": "tsx src/__tests__/test-runner.ts --critical-only",
    "test:performance": "tsx src/__tests__/test-runner.ts --performance",
    "test:auth": "jest src/__tests__/auth --verbose",
    "test:api": "jest src/__tests__/api --verbose",
    "test:components": "jest src/__tests__/components --verbose",
    "test:e2e": "jest src/__tests__/e2e --verbose --testTimeout=120000",
    "test:unit": "jest src/__tests__/lib --verbose",
    "test:ci": "npm run test:critical && npm run test:coverage",
    "test:e2e:prod": "E2E_BASE_URL=$NEXTAUTH_URL playwright test",
    "test:e2e:local": "E2E_BASE_URL=http://localhost:3000 playwright test"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@prisma/client": "^6.13.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.5",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.1",
    "@radix-ui/react-toggle": "^1.1.9",
    "@sentry/nextjs": "^10.1.0",
    "@stripe/stripe-js": "^7.8.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.54.0",
    "@types/bcryptjs": "^2.4.6",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "gray-matter": "^4.0.3",
    "iconv-lite": "^0.6.3",
    "lucide-react": "^0.469.0",
    "next": "^14.2.15",
    "pdfkit": "^0.15.0",
    "posthog-js": "^1.258.5",
    "prisma": "^6.13.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.5.2",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.1",
    "remark-html": "^16.0.1",
    "resend": "^4.8.0",
    "stripe": "^18.4.0",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@faker-js/faker": "^9.9.0",
    "@playwright/test": "^1.54.2",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.17.10",
    "@types/pdfkit": "^0.13.5",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.15",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.4.1",
    "tsx": "^4.20.3",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": "22.x",
    "npm": ">=10.0.0"
  }
}
